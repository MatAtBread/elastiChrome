{
	"manifest_version": 2,
	"name": "ElasticHQ Extension",
	"description": "www.elastichq.org",
	"version": "1.1",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_security_policy": "script-src 'self' https://ajaxorg.github.io; object-src 'self'",
	"permissions": [
		"tabs",
		"activeTab",
		"storage"
	]
}
